<view class="page_hd" style="text-align:center">
<text class="page_title"> 教练课表 </text>
</view>

<!--page/one/index.wxml-->
<view class="page">
    <view class="nav-son" bindtap="listpx1">
        <view>张教练（羽毛球）</view>
        <image src='{{imgUrl}}'></image>
    </view>
 <view class="temp {{pxopen1 ? 'slidown' : 'slidup'}} {{pxshow1 ? 'disappear':''}}">
        <view wx:for="{{ymq1}}">
            {{item}}
        </view>
    </view>
    <view class="nav-son" bindtap="listpx2">
        <view>周教练（羽毛球）</view>
        <image src='{{imgUrl}}'></image>
    </view>
 <view class="temp {{pxopen2 ? 'slidown' : 'slidup'}} {{pxshow2 ? 'disappear':''}}">
        <view wx:for="{{ymq2}}">
            {{item}}
        </view>
    </view>
     <view class="nav-son" bindtap="listpx3">
        <view>张教练（乒乓球）</view>
        <image src='{{imgUrl}}'></image>
    </view>
 <view class="temp {{pxopen3 ? 'slidown' : 'slidup'}} {{pxshow3 ? 'disappear':''}}">
        <view wx:for="{{ppq}}">
            {{item}}
        </view>
    </view>
</view>
